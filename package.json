    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "vue-template-compiler": "^2.6.11",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.0"
